{
  "manifest_version": 2,
  "description": "A Google Play Music App",
  "name": "Google Play Music",
  "version": "1",
  "minimum_chrome_version": "26",
  "permissions": [
    "webview",
    "https://play.google.com/"
  ],
  "offline_enabled": false,
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "icons": {
    "16": "images/icon.webp",
    "48": "images/icon.webp",
    "128": "images/icon_128.png"
  }
}
